<Window x:Class="ызщке.DeleteAthleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Удалить спортсмена" Height="350" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Выберите спортсмена для удаления:"/>
            <ComboBox x:Name="comboBoxAthletes" Width="300" Margin="0,10,0,10"/>

            <Button Content="Удалить" Width="100" Click="btnDeleteAthlete_Click"/>
        </StackPanel>
    </Grid>
</Window>
