<Window x:Class="ызщке.SectionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Секции" Height="350" Width="336">
    <Grid RenderTransformOrigin="0.828,0.591" Margin="0,0,10,0">
        <ComboBox x:Name="comboBoxSections" HorizontalAlignment="Left" VerticalAlignment="Top" Width="300" Margin="10,10,0,0" SelectionChanged="comboBoxSections_SelectionChanged"/>
        <TextBlock x:Name="textBlockTrainerInfo" HorizontalAlignment="Left" VerticalAlignment="Top" Width="300" Margin="10,40,0,0" />
        <ListBox x:Name="listBoxAthletes" HorizontalAlignment="Left" VerticalAlignment="Top" Width="300" Height="200" Margin="10,70,0,0"/>
        <Button x:Name="btnPrintPDF" Content="Распечатать в PDF" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" Height="30" Margin="10,280,0,0"  Click="btnPrintPdf_Click"/>
    </Grid>
</Window>
